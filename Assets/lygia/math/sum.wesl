fn sum(v: f32 ) -> f32 { return v; }
fn sum2(v: vec2f ) -> f32 { return v.x+v.y; }
fn sum3(v: vec3f ) -> f32 { return v.x+v.y+v.z; }
fn sum4(v: vec4f ) -> f32 { return v.x+v.y+v.z+v.w; }

